{
  "name": "messenger",
  "version": "0.0.1",
  "versionCode": 4,
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "android-stage": "yarn mobile env-stage && yarn mobile android",
    "android-rc": "yarn mobile env-rc && yarn mobile android",
    "android": "yarn mobile env && yarn mobile android",
    "android-build-stage": "yarn mobile env-stage && yarn mobile android --variant=release",
    "android-build-rc": "yarn mobile env-rc && yarn mobile android --variant=release",
    "android-build": "yarn mobile env && yarn mobile android --variant=release",
    "pwa-build-production": "yarn pwa build-production",
    "pwa-build-stage": "yarn pwa build-stage",
    "pwa-build-rc": "yarn pwa build-rc",
    "compile": "tsc -b --incremental",
    "ios": "yarn workspace mobile  ios",
    "studio": "yarn workspace mobile studio",
    "xcode": "yarn workspace mobile xcode",
    "components": "yarn workspace components",
    "core": "yarn workspace core",
    "desktop": "set PORT=3001 && yarn workspace desktop",
    "pwa": "set PORT=3002 && yarn workspace pwa",
    "web": "set PORT=3003 && yarn workspace web",
    "mobile": "yarn workspace mobile",
    "refresh-swag": "yarn swag-ts",
    "prettierAll": "prettier --write .",
    "lint": "eslint --ext .js ./",
    "test": "jest",
    "windows": "react-native run-windows"
  },
  "dependencies": {
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "0.72.6",
    "react-native-svg": "^14.1.0"
  },
  "devDependencies": {
    "babel-loader": "^9.1.3",
    "babel-plugin-danger-remove-unused-import": "^2.0.0",
    "@babel/core": "^7.23.9",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/generator": "^7.23.6",
    "@babel/compat-data": "^7.23.5",
    "@babel/helper-compilation-targets": "^7.23.6",
    "@babel/helper-create-class-features-plugin": "^7.23.10",
    "@babel/helper-module-transforms": "^7.23.3",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-typescript": "^7.23.3",
    "@babel/preset-react": "^7.23.3",
    "@babel/runtime": "^7.23.9",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@types/react-native-push-notification": "^8.1.4",
    "commitlint": "^18.0.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.52.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "metro-config": "^0.76.8",
    "prettier": "^3.0.3",
    "prettier-plugin-jsdoc": "^1.1.1",
    "react-test-renderer": "^18.2.0",
    "swagger-typescript": "^6.5.0",
    "ts-loader": "^9.5.1",
    "typescript": "4.8.4",
    "@microsoft/signalr": "^8.0.0",
    "socket.io-client": "^4.7.4",
    "socket.io": "^4.7.4"
  }
}
